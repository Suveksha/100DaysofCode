import java.util.*;
public class EvenOrOdd
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int num=sc.nextInt();
int arr[]=new int[num];
for(int i=0;i<num;i++)
arr[i]=sc.nextInt();

int sum=0;
int prod=1;
for(int a=0;a<num;a++)
{
sum+=arr[a];
prod=prod*arr[a];
}

if(sum%2==0)
System.out.println("Sum="+sum);
else
System.out.println("Product="+prod);
}
}
